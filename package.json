{
  "author": "Paul Serby <paul@serby.net>",
  "name": "uber-cache",
  "description": "Async in-memory that sets the interface for a number of uber-* caching engine: Memcached, Redis, MongoDB, LevelDB",
  "version": "2.1.4",
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "tags": [
    "cache",
    "memory",
    "ttl",
    "lru"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:serby/uber-cache"
  },
  "main": "./uber-cache.js",
  "scripts": {
    "lint": "jshint . --reporter=./node_modules/jshint-full-path/index.js",
    "pretest": "npm run-script lint",
    "bench": "node benchmark/uber-cache.bench",
    "test": "istanbul cover ./node_modules/.bin/_mocha -- -R spec -r should --recursive test/**/*.test.js",
    "posttest": "istanbul check-coverage && rm -rf coverage"
  },
  "engines": {
    "node": ">=0.10"
  },
  "dependencies": {
    "async": "^1.5.2",
    "lodash.assign": "^4.0.9",
    "lru-cache": "^4.0.1",
    "through": "^2.3.8"
  },
  "devDependencies": {
    "istanbul": "^0.4.4",
    "jshint": "^2.9.2",
    "jshint-full-path": "^1.1.1",
    "mocha": "^2.5.3",
    "should": "^9.0.2",
    "stream-assert": "^2.0.3"
  }
}
